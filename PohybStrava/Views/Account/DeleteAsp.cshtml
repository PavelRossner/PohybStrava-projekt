@model PohybStrava.Models.ApplicationUser


@{
    ViewData["Title"] = "Smazat uživatele z databáze";
}

<body>

    <div id="sirkatextu">
        <h3>Opravdu chcete uživatele smazat?</h3>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Id)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Id)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Email)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Email)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Jmeno)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Jmeno)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Prijmeni)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Prijmeni)
            </dd>
        </dl>

        <form asp-action="DeleteAsp">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Smazat" class="pridat-tlacitko" /> |
            <a asp-action="Administration" class="pridat-tlacitko">Zpět</a>
        </form>
    </div>
</body>